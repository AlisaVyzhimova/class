#задача 1 
p=int(input())
a=2
while a<p:
    a+=1
    print(a)

#задача 2
a=int(input())
b=int(input())
while a<b:
    a+=1
    print(a)

#задача 3
a=int(input())
b=int(input())
while  a<b:
    b-=1
    print(b)
#Задача 4
N=int(input())
K=int(input())
while K*3>=N:
   K-=1 
   print(K)
#Задача 5
N=int(input())
K=int(input())
while K*3<=N:
   K+=1 
   print(K)

